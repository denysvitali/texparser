<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>
  <skipIfNotSupported/>
  <programGroup defaultName="DickimawBooks\TeXParserApp"
                location="applications||startMenu"/>
  <shortcut
    name="Start TeXParserApp"
    target="$INSTALL_PATH\bin\texparserappgui.bat"
    commandLine=""
    workingDirectory="$INSTALL_PATH"
    description="This starts TeXParserApp"
    iconFile="$INSTALL_PATH\lib\icons\texparser.ico"
    iconIndex="0"
    initialState="noShow||normal||maximized||minimized"
    programGroup="yes||no"
    desktop="yes||no"
    applications="yes||no"
    startMenu="yes||no"
    startup="yes||no">

    <createForPack name="TeXParserApp Batchfiles"/>
  </shortcut>
</shortcuts>
