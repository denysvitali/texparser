<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="DickimawBooks/TeXParserApp"
  location="applications"/>

  <shortcut
     name="TeXParserApp"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
     startup="no"
     target="$INSTALL_PATH/bin/texparserapp"
     commandLine=""
     workingDirectory="$INSTALL_PATH/bin"
     description="(La)TeX Parser"
     iconFile="$INSTALL_PATH/lib/icons/texparser-small.png"
     iconIndex="0"
     type="Application"
     encoding="UTF-8"
     terminal="true"
     KdeSubstUID="false"
     initialState="normal">
     <createForPack name="Core"/>
   </shortcut>

   <shortcut
     name="Documentation"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
     startup="no"
     target="okular"
     workingDirectory=""
     commandLine="$INSTALL_PATH/doc/texparser.pdf"
     initialState="noShow"
     iconFile="acroread"
     iconIndex="0"
     url="$INSTALL_PATH/doc/texparser.pdf"
     type="Application"
     encoding="UTF-8"
     description="TeXParserApp user documentation (PDF format)">

     <createForPack name="Documentation-PDF"/>
   </shortcut>

   <shortcut
     name="Uninstaller"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/lib/java/bin/java"
     commandLine="-jar
     &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
     initialState="noShow"
     iconFile="trashcan_full"
     iconIndex="0"
     workingDirectory=""
     type="Application"
     encoding="UTF-8"
     description="TeXParserApp uninstaller">
     <createForPack name="Core" />
   </shortcut>

</shortcuts>
